{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**", "package.json", "package-lock.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "src/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/socket.io/.*",
      "dest": "src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "key": "Cache-Control",
      "value": "public, max-age=31536000, immutable",
      "source": "/assets/.*"
    },
    {
      "key": "Cache-Control",
      "value": "public, max-age=86400",
      "source": "/favicon.ico"
    }
  ]
}