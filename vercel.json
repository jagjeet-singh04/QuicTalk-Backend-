



{
"version": 2,
"builds": [
{
"src": "src/index.js",
"use": "@vercel/node",
"config": {
"includeFiles": ["src/**", "package.json", "package-l.json"]
}
}
],
"routes": [
{
"src": "/api/(.)",
"dest": "src/index.js",
"methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
},
{
"src": "/socket.io/(.)",
"dest": "src/index.js"
},
{
"src": "/(.*)",
"dest": "/"
}
]
}